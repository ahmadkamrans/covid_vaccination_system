<p id="notice"><%= notice %></p>

<h1>Vaccines</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>No of doses</th>
      <th>Days to sec dose</th>
      <th>Amount</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_vaccines.each do |vaccine| %>
      <tr>
        <td><%= vaccine.name %></td>
        <td><%= vaccine.no_of_doses %></td>
        <td><%= vaccine.days_to_sec_dose %></td>
        <td><%= vaccine.amount %></td>
        <td><%= link_to 'Show', vaccine %></td>
        <td><%= link_to 'Edit', edit_admin_vaccine_path(vaccine) %></td>
        <td><%= button_to 'Destroy', admin_vaccine_path(vaccine), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vaccine', new_admin_vaccine_path %>
